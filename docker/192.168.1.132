version: '3.3'
services:
  httpd:
    image: "httpd:${TAG}"
    network_mode: host
  httpd2:
    image: "httpd:${TAG}"
  httpd3:
    image: "httpd:${TAG}"
  nifi1:
    image: apache/nifi:1.11.4
    ports:
      - 8080:8080
    environment:
      - NIFI_WEB_HTTP_PORT=8080
      - NIFI_ZK_CONNECT_STRING=zookeeper:2181
      - NIFI_ELECTION_MAX_WAIT=1 min
    volumes:
    - /input:/input
    - /output:/output

